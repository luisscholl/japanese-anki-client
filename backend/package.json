{
  "name": "villosum",
  "productName": "villosum",
  "version": "1.0.0",
  "description": "Villosum Backend",
  "main": "main.js",
  "scripts": {
    "postinstall": "npm i -g pm2",
    "start": "node -e \"const fs = require('fs'); fs.copyFileSync('./local.environment.json', './environment.json');\" && nodemon main.js",
    "prodEnvTest": "node -e \"const fs = require('fs'); fs.copyFileSync('./prod.environment.json', './environment.json');\" && nodemon main.js",
    "prod": "node -e \"const fs = require('fs'); fs.copyFileSync('./prod.environment.json', './environment.json');\" && pm2 restart main",
    "lint": "echo \"No linting configured\"",
    "deploy": "ssh schatten \"mkdir tmp\" && scp -r * schatten:~/tmp && ssh schatten \"rsync -av --delete tmp/ villosum-backend && cd villosum-backend && mv prod.environment.json environment.json && pm2 restart villosum-backend && cd .. && rm -r tmp\""
  },
  "keywords": [],
  "author": {
    "name": "Luis Scholl",
    "email": "ssh@luisscholl.de"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "cognito-express": "^2.0.19",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-openid-connect": "^2.5.0",
    "express-session": "^1.17.2",
    "keycloak-connect": "^15.0.2",
    "peer": "^0.6.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
