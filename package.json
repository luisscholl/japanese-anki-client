{
  "name": "villosum",
  "productName": "villosum",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "main.js",
  "scripts": {
    "start": "concurrently -n Backend,Frontend \"nodemon main.js --config nodemon.json\" \"cd client && npm start\"",
    "make": "cd client && npm run build && cd .. && nexe main.js -r client/dist/client --build",
    "lint": "echo \"No linting configured\"",
    "install": "git submodule update --init --recursive && npm i -g concurrently && npm i -g @angular/cli && cd client && npm install && cd ..",
    "postinstall": ".\\node_modules\\.bin\\electron-rebuild.cmd"
  },
  "keywords": [],
  "author": {
    "name": "Luis Scholl",
    "email": "ssh@luisscholl.de"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-pouchdb": "^4.2.0",
    "leveldown": "^5.6.0",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-leveldb": "^7.2.2",
    "pouchdb-users": "^1.0.6",
    "qrcode": "^1.4.4"
  },
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "concurrently": "^6.2.1",
    "nexe": "^4.0.0-beta.18",
    "nodemon": "^2.0.12"
  }
}
