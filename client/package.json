{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --ssl",
    "build": "npm run makeLineEndingsConsistent && npm run generateAttributions && ng build --configuration=production --base-href /",
    "postinstall": "npm run getJaSVGs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2ea",
    "generateAttributions": "npm run generateClientAttributions && npm run generateBackendAttributions && npm run generateGenericAttributions",
    "generateClientAttributions": "generate-attribution && move-file oss-attribution/attribution.txt ../licenses/client-licenses.txt && rimraf oss-attribution",
    "generateBackendAttributions": "cd ../backend && generate-attribution && move-file oss-attribution/attribution.txt ../licenses/backend-licenses.txt && rimraf oss-attribution && cd ../client",
    "generateGenericAttributions": "cd .. && generate-attribution && move-file oss-attribution/attribution.txt licenses/generic-licenses.txt && rimraf oss-attribution && cd client",
    "getJaSVGs": "copyfiles -f \"../animCJK/svgsJa/*\" src/assets/jaSVGs && copyfiles -f \"../animCJK/svgsJaSpecial/*\" src/assets/jaSVGs && copyfiles -f \"../animCJK/svgsKana/*\" src/assets/jaSVGs",
    "makeLineEndingsConsistent": "sfk remcr -dir . -yes",
    "deploy": "npm run build && scp -r dist/client licht:~ && ssh licht \"sudo rsync -av --delete client/ /var/www/html/villosum\" && ssh licht \"rm -r client/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/localize": "^12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@angular/service-worker": "~12.2.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngx-pwa/local-storage": "^12.1.0",
    "angular-oauth2-oidc": "^12.1.0",
    "buffer": "^6.0.3",
    "bufferutil": "^4.0.3",
    "memorystream": "^0.3.1",
    "object-sizeof": "^1.6.1",
    "peerjs": "^1.3.2",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-replication-stream": "^1.2.9",
    "process": "^0.11.10",
    "rxjs": "~7.3.0",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.3.1",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@angular/language-service": "~12.2.4",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.7.13",
    "@types/pouchdb": "^6.4.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "move-file": "^3.0.0",
    "generate-attribution": "^1.7.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "source-map": "^0.7.3",
    "ts-node": "~10.2.1",
    "typescript": "~4.3.5"
  }
}
