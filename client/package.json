{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "npm run makeLineEndingsConsistent && npm run generateAttributions && ng build --prod --base-href /japanese-anki-client/client/dist/client/",
    "install": "npm run getJaSVGs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2ea",
    "generateAttributions": "npx oss-attribution-generator && npx move-file-cli oss-attribution/attribution.txt ../licenses/npm-licenses.txt && npx rimraf oss-attribution",
    "getJaSVGs": "npx copyfiles -f \"../animCJK/svgsJa/*\" src/assets/jaSVGs && npx copyfiles -f \"../animCJK/svgsJaSpecial/*\" src/assets/jaSVGs && npx copyfiles -f \"../animCJK/svgsKana/*\" src/assets/jaSVGs",
    "makeLineEndingsConsistent": "sfk remcr -dir . -yes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/localize": "^11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@angular/service-worker": "~11.2.9",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ngx-pwa/local-storage": "^11.1.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.8",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "@angular/language-service": "~11.2.9",
    "@types/jasmine": "~3.6.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.37",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.0"
  }
}
